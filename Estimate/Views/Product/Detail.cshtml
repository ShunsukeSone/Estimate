    <div class="text" style="padding: 10px; margin-bottom: 10px; border: 1px solid #333333;">
        <div class="">最小</div>
        <div class="">
            <div class="text">区分</div>
            <div class="text"><input type="text" id="" name="" value="@Model.MIN_Kubun"/></div>
