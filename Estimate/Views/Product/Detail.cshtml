            <div class=""><input type="text" id="" name="" value="@Model.MAX_Tanni"/></div>

            <div class="text">係数</div>
            <div class=""><input type="text" id="" name="" value="@Model.MAX_Keisu"/></div>

            <div class="text">入数</div>
            <div class=""><input type="text" id="" name="" @Model.MAX_Nyusu/></div>
        </div>
    </div>

    @*中間区分*@
    <div class="text" style="padding: 10px; margin-bottom: 10px; border: 1px solid #333333;">
